<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iron.mapper.TestMapper">


    <select id="getUser" resultType="com.iron.pojo.User">
        select * from user
